    public void fireTheLauncher() {
    	launcherESC.set(-1);
    	Timer.delay(0.5);
        brushESC.set(1);
    	
    }
    
    
    public void stoplauncher(){
    	  brushESC.set(0);
        Timer.delay(0.5);
        launcherESC.set(0);
    	
       
        
    }


public void rampUp() {
	launcherESC.set(-0.25);
	Timer.delay(0.25);
	launcherESC.set(-0.5);
	Timer.delay(0.25);
	launcherESC.set(-0.75);
	Timer.delay(0.25);
	launcherESC.set(-1);
	
	
}
	
